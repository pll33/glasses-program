{
  "manifest_version": 2,
  "name": "Glasses Inventory",
  "short_name": "Glasses Inventory",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["js/background.js"]
    },
    "launch": {
      "local_path": "index.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ]
}
